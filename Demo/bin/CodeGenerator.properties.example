#
# Connection information
#
jdbcURL = jdbc:oracle:thin:@(DESCRIPTION = (ADDRESS_LIST = (ADDRESS = (COMMUNITY = tcp.world) (PROTOCOL = TCP) (Host = <YOUR HOST>) (Port = 1521))) (CONNECT_DATA = (SID = <YOUR_SID>)))
jdbcUser = core
jdbcPassword = core

# Output for generated source
resultDir = ../src

#
# Abstract Mode
#
# Make generated classes abstact
# Use to subclass generated objects to add your own annotations (transaction attributes, security, etc)
# Default false
# Options true or false

#abstract=true

#
# Target Platform
#
# Use to subclass generated objects to add your own annotations (transaction attributes, security, etc)
# Default default
# Options: oc4j1013, default
#    oc4j1013 - annotations, for oc4j10.1.3.5.0 EJB 3.0
#    default - no annotations, can be run with plain jdbc

#targetPlatform=oc4j1013

#
# Template Dir
#
# Optional override for templates
# Use if you want to make your own  templates (if you want to make your own for a different platform or methods etc).
# Default GeneratorHome/src/templates

#templateDir = ../../Generator/src/templates

#
# XmlSQL file
#
# Optional override for sql that generates xml package metadata 
# Use  if you want to make your own with different naming, or filters etc
# Default GeneratorHome/src/sql/bridge_generate_xml.sql

#generateXmlSql = ../../Generator/src/sql/bridge_generate_xml.sql

